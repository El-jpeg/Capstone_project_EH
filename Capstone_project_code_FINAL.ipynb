        "df_exp = df_exp[df_exp[\"Quintile\"].apply(lambda x: str(x).isdigit())]\n",
        "df_exp[\"Quintile\"] = df_exp[\"Quintile\"].astype(int)\n",
        "\n",
        "# Keep only needed columns\n",
        "exp_df = df_exp[[\"Country\",\"Year\",\"Quintile\",\"MeanExpenditure\"]].copy()\n",
        "\n",
        "print(exp_df.head())\n"
